    "files.autoSave": "afterDelay",
    "files.watcherExclude": {
        "**/.git/**": true,
        "**/.git/objects/**": false,
        "**/.git/subtree-cache/**": false,
        "**/node_modules/**": false,
        "**/node_modules/*/**": true
    },
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
}
